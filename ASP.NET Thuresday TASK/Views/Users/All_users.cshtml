@model IEnumerable<ASP.NET_Thuresday_TASK.Models.User>

<h1>All Users</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <!-- Add other columns as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Name</td>
                <td>@user.Email</td>
                <!-- Add other columns as needed -->
            </tr>
        }
    </tbody>
</table>
<a asp-controller="Users" asp-action="Create">Create New User</a>
